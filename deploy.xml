<?xml version="1.0" encoding="UTF-8"?><project name="deploy-tool-zp-sea" default="deploy.all">    <property file="deploy.properties"/>    <property name="socket.private" location="${deploymentRoot}${deployment.private}"/>    <property name="socket.live" location="${deploymentRoot}${deployment.live}"/>    <!-- Copy to new target -->    <target name="deploy.transferFile" description="Transfer files ${gameName}">        <delete dir="${socketDir}/res"/>        <delete dir="${socketDir}/lib"/>        <delete dir="${socketDir}/${jarFile}"/>        <copy file="${artifact.output.release}/${jarFile}" tofile="${socketDir}/${jarFile}"/>        <mkdir dir="${socketDir}/res"/>        <copy todir="${socketDir}/res">            <fileset dir="${artifact.output.release}/res"/>        </copy>        <mkdir dir="${socketDir}/lib"/>        <copy todir="${socketDir}/lib">            <fileset dir="${artifact.output.release}/lib"/>        </copy>    </target>    <target name="predeploy" description="Create socket Dir" depends="socketDir.check" if="dir.exists">        <mkdir dir="${socketDir}"/>    </target>    <target name="socketDir.check">        <condition property="dir.exists" else="false">            <available file="${socketDir}" type="dir"/>        </condition>    </target>    <target name="deploy.private" description="Deploy ${gameName} ">        <antcall target="predeploy">            <param name="socketDir" value="${socket.private}"/>        </antcall>        <antcall target="deploy.transferFile">            <param name="socketDir" value="${socket.private}"/>        </antcall>    </target>    <target name="deploy.live" description="Deploy ${gameName} ">        <antcall target="predeploy">            <param name="socketDir" value="${socket.live}"/>        </antcall>        <antcall target="deploy.transferFile">            <param name="socketDir" value="${socket.private}"/>        </antcall>    </target>    <!-- Comment -->    <target name="deploy.all" depends="deploy.private, deploy.live" description="deploy all"/></project>